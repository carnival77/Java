package chap06.class1;

public class Test {
	String s;

	public Test(String st) {
		s = st;
	}

}

class Child extends Test {
	String s;

	public Child(String st) {
		super(st);
		s = st;
		System.out.println(s);
	}
}

class Test {
	public static void main(String[] args) {
		Child child = new Child("HI");
		Test parent = new Test("HELLO");
	}
}