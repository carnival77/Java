package chap06.class1;

class Test {
	public static void main(String[] args) {
		Child child = new Child("HI");
		Test parent = new Test("HELLO");
	}
}


public class Parent {
	String s;

	public Parent(String st) {
		s = st;
	}

}

class Child extends Parent {
	String s;

	public Child(String st) {
		super(st);
		s = st;
		System.out.println(s);
	}
}