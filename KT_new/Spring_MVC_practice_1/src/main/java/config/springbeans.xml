<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<bean id="strPrinter" class="com.mypractice.first.strPrinter"></bean>

	<bean id="conPrinter" class="com.mypractice.first.conPrinter"></bean>

	<bean id="hello" class="com.mypractice.first.Hello">
		<property name="name" value="스프링"></property>
		<property name="printer" ref="strPrinter"></property>
	</bean>
	
	






</beans>
